const date = new Date();
console.log(date); // 현재 시간

const date1 = new Date(2025, 11, 25); // Thu Dec 25 2025 00:00:00 GMT+0900 (한국 표준시)
const date2 = new Date(2025, 11, 25, 18, 30, 50); // Thu Dec 25 2025 18:30:50 GMT+0900 (한국 표준시)
console.log(date1);
console.log(date2);

const date3 = new Date("2025-12-25"); // Thu Dec 25 2025 09:00:00 GMT+0900 (한국 표준시)
console.log(date3);
const date4 = new Date("2025/12/25/12:24:50"); // Thu Dec 25 2025 12:24:50 GMT+0900 (한국 표준시)
console.log(date4);
